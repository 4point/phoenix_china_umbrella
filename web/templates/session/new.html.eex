<div class="phoenix_china-form">

  <div class="row">

    <div class="col-sm-8">
      <h2>欢迎登录Phoenix中文社区</h2>
      <%= render "form.html",
        changeset: @changeset,
        action: session_path(@conn, :create),
        conn: @conn
      %>
    </div>

    <div class="col-sm-4">
      <h2>第三方登录</h2>
      <ul>
        <li><%= link raw("<i class=\"fa fa-github fa-2x\"></i> Github"), to: auth_path(@conn, :request, "github") %></li>
      </ul>
    </div>


  </div>


</div>
